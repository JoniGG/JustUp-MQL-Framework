//--- Time frames enumeration
#define _TIMEFRAMES_ENUMERATION_VERSION_ 1.0

enum Timeframes
{
   TF_CURRENT = -1, // current period
    TF_M1 = 0,       // 1 minute
    TF_M5 = 1,       // 5 minutes
    TF_M15 = 2,      // 15 minutes
    TF_M30 = 3,      // 30 minutes
    TF_H1 = 4,       // 1 hour
    TF_H4 = 5,       // 4 hours
    TF_D1 = 6,       // 1 day
    TF_W1 = 7,       // 1 week
    TF_MN1 = 8       // 1 month
};